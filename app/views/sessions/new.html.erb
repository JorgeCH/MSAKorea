<div class="main" id="two">
  <div class="container">
    <div class="log">

<h1>Log In</h1>
<%= form_for :signin, method: "POST" do |f| %>
	<p>
	<%= f.label :email %><br />
	<%= f.text_field :email, :placeholder => "Email" %>
	</p>
	<p>
	<%= f.label :password %><br />
	<%= f.password_field :password, :placeholder => "Password"%>
	</p>
	<%= f.submit "Log In" %>
<% end %>

    </div>
  </div>
</div>